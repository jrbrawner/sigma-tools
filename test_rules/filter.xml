<Filter>
    <FilterName>FailedLoginFilter</FilterName>
    <Description>Filter for Failed Login Events</Description>
    <Active>true</Active>
    <EventFilter>true</EventFilter>
    <FilterExpression>
        <Operator>AND</Operator>
        <FilterExpression>
            <Field>EventID</Field>
            <Operator>EQUALS</Operator>
            <Value>1001</Value>
        </FilterExpression>
        <FilterExpression>
            <Field>Outcome</Field>
            <Operator>EQUALS</Operator>
            <Value>Failed</Value>
        </FilterExpression>
    </FilterExpression>
</Filter>
